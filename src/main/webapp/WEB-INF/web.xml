<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" 
         version="2.5">
  <servlet>
    <servlet-name>FeedMe</servlet-name>
    <servlet-class>feedMe.FeedMeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FeedMe</servlet-name>
    <url-pattern>/FeedMe</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>RandomRecipes</servlet-name>
    <servlet-class>feedMe.RandomRecipesServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RandomRecipes</servlet-name>
    <url-pattern>/random</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>home.jsp</welcome-file>
  </welcome-file-list>
</web-app>